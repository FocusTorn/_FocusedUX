{
    "name": "@fux/shared-services",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/_shared/shared-services/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:swc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "packages/_shared/shared-services/dist",
                "main": "packages/_shared/shared-services/src/index.ts",
                "tsConfig": "packages/_shared/shared-services/tsconfig.lib.json",
                "stripLeadingPaths": true,
                "assets": [],
